{
  "sources": [
    "**/*.q"
  ],
  "dataFiles": [],
  "data": {},
  "plugins": {
    "@breuleux/quaint-my-utils": {
      "defaultLanguage": null,
      "highlightTheme": "xcode",
      "language": "english"
    },
    "google-analytics": {
      "trackingId": "UA-42450029-1",
      "auto": true
    },
    "earlgrey": {},
    "disqus": {
      "shortname": "breuleux",
      "idField": "id"
    },
    "share-buttons": {},
    "look-nice": {
      "useNav": true,
      "fonts": {
        "body": "g:Open Sans",
        "title": "g:Mate SC"
      },
      "widths": {
        "body": "750px"
      },
      "colors": {
        "link-visited": "rgb(85, 26, 139)"
      },
      "backgrounds": {
        "body": "#fff",
        "nav": "#fff",
        "table": "#fff",
        "table-odd": "#fff",
        "table-even": "#fff",
        "table-header": "#000"
      }
    },
    "repple": {}
  },
  "inject": [],
  "contentRoot": "content",
  "output": "out",
  "format": "html",
  "templates": {
    "default": "@minimal",
    "root": "template"
  },
  "resources": {
    "root": "resources",
    "inline": false,
    "files": {
      "blog.sass": "style/blog.sass"
    },
    "url": "resources"
  },
  "meta": {
    "file": "content/meta.json",
    "sortBy": "-date"
  },
  "watch": false,
  "hostname": "http://breuleux.net",
  "siteRoot": "blog/",
  "server": {
    "enable": true,
    "delay": 300,
    "port": 9135,
    "startPage": "index.html",
    "openBrowser": true
  },
  "verbose": false,
  "files": [
    "assets"
  ]
}
